    {
        $this->resource(User::class)
            ->heading('Recent Users')
            ->timestamp()
            ->viewAll();
    }

    /**
     * Get the URI key for the metric.
     *
     * @return string
     */
    public function uriKey()
    {
        return 'uri-key';
    }
}